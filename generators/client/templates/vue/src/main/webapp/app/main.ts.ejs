new Vue({
  el: '#app',
  components: { App },
  template: '<App/>',
  router,
  provide: {
    activateService: () => activateService,
    auditsService: () => auditsService,
    healthService: () => healthService,
    loginModalService: () => loginModalService,
    <%_ if (applicationType === 'gateway' && serviceDiscoveryType) { %>
