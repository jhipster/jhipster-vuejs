    <%_ } %>
    configurationService: () => configurationService,
    logsService: () => logsService,
    metricsService: () => metricsService,
    principal: () => principal,
    registerService: () => registerService,
    <%_ if (websocket === 'spring-websocket') { _%>
    trackerService: () => trackerService,
    <%_ } _%>
