const <%= entityAngularName %> = {
    mixins: [Principal, <%= entityAngularName %>Service<% if (fieldsContainBlob) { %>, JhiDataUtils<% } %>],
    data() {
      return {
          removeId: null,
          <%= entityInstance %>s: []
