{
  "name": "generator-jhipster-vuejs",
  "version": "1.8.2",
  "description": "A Vue.js blueprint for JHipster. It will use Vue.js as the frontend library",
  "keywords": [
    "yeoman-generator",
    "jhipster-blueprint",
    "jhipster-5"
  ],
  "homepage": "https://github.com/jhipster/jhipster-vuejs",
  "author": {
    "name": "JHipster Team",
    "email": "d4udts@gmail.com",
    "url": "https://jhipster.tech"
  },
  "files": [
    "generators"
  ],
  "main": "generators/app/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jhipster/jhipster-vuejs.git"
  },
  "dependencies": {
    "chalk": "2.4.1",
    "eslint-config-airbnb": "17.1.0",
    "generator-jhipster": "6.10.0",
    "jhipster-core": "7.3.4",
    "lodash": "4.17.13",
    "mkdirp": "0.5.1",
    "pluralize": "7.0.0",
    "prettier": "1.13.5",
    "randexp": "0.4.9",
    "shelljs": "0.8.2"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "ejs-lint": "0.3.0",
    "eslint": "5.14.0",
    "eslint-config-airbnb-base": "13.0.0",
    "eslint-plugin-import": "2.12.0",
    "fs-extra": "6.0.1",
    "inquirer": "7.0.4",
    "jsdoc": "3.6.3",
    "mocha": "5.2.0",
    "sinon": "6.0.0",
    "yeoman-assert": "3.1.1",
    "yeoman-test": "2.2.0"
  },
  "resolutions": {
    "inquirer": "7.0.4"
  },
  "scripts": {
    "lint": "eslint .",
    "lint-fix": "npm run lint -- --fix",
    "pretest": "npm run lint",
    "test": "npm run test:unit -- test/*.spec.js",
    "test:unit": "mocha --timeout 60000 --slow 0 --reporter spec",
    "jsdoc": "jsdoc --configure jsdoc-conf.json",
    "release": "git push && git push --tags && npm publish",
    "release:patch": "npm version patch -a -m \"Update to %s\" && npm run release",
    "release:minor": "npm version minor -a -m \"Update to %s\" && npm run release",
    "release:major": "npm version major -a -m \"Update to %s\" && npm run release"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/jhipster/jhipster-vuejs/issues"
  }
}
